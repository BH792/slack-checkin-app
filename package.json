{
  "name": "slack-checkin-app",
  "version": "0.0.0",
  "private": true,
  "description": "Enable checkin command in slack",
  "scripts": {
    "start:dev": "DEBUG=slack-checkin-app:* nodemon --exec 'node -r dotenv/config' ./bin/www",
    "start": "node ./bin/www",
    "db:create": "sequelize db:create",
    "db:migrate": "sequelize db:migrate",
    "db:seed:all": "sequelize db:seed:all",
    "db:seed:undo:all": "sequelize db:seed:undo:all"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BH792/slack-checkin-app.git"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "body-parser": "~1.17.1",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.3",
    "ejs": "~2.5.6",
    "express": "~4.15.2",
    "morgan": "~1.8.1",
    "node-fetch": "^1.7.3",
    "pg": "<7.0.0",
    "pg-hstore": "^2.3.2",
    "sequelize": "^4.13.5",
    "sequelize-cli": "^3.0.0",
    "serve-favicon": "~2.4.2"
  },
  "devDependencies": {
    "dotenv": "^4.0.0",
    "nodemon": "^1.12.1"
  }
}
